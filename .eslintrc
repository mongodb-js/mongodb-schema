{
  "parserOptions": {
    "ecmaVersion": 2019
  },
  "env": {
    "mocha": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "expect": true,
    "sinon": true
  },
  "rules": {
    "camelcase": 1,
    "new-cap": 0,
    "no-use-before-define": 0
  },
  "extends": [
    "mongodb-js/node",
    "mongodb-js/browser"
  ]
}
